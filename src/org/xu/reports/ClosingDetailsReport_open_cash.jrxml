<!DOCTYPE jasperReport PUBLIC "//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport
		 name="open_cash"
		 columnCount="1"
		 printOrder="Vertical"
		 orientation="Landscape"
		 pageWidth="200"
		 pageHeight="30"
		 columnWidth="200"
		 columnSpacing="0"
		 leftMargin="0"
		 rightMargin="0"
		 topMargin="0"
		 bottomMargin="0"
		 whenNoDataType="AllSectionsNoDetail"
		 isTitleNewPage="false"
		 isSummaryNewPage="false">
	<property name="ireport.scriptlethandling" value="0" />
	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="CUR_DATE" isForPrompting="true" class="java.lang.String"/>
	<parameter name="OpenCashSubReport" isForPrompting="false" class="net.sf.jasperreports.engine.JasperReport"/>
	<queryString><![CDATA[select
 (cd_.pennies/100 +
cd_.nickels/20+
cd_.dimes/10 +
cd_.quarters/4 +
cd_.half_dollars/2 +
cd_.dollars +
cd_.singles +
cd_.fives*5 +
cd_.tens*10 +
cd_.twenties*20 +
cd_.fifties*50 +
cd_.hundreds*100) as 'open_cash'
from cash_drawing cd_
where DATE(cd_.`date`) = DATE($P{CUR_DATE}) and cd_.`location_id` = 1 and cd_.`openClose`=0
ORDER BY openCLOSE DESC
 LIMIT 0,1]]></queryString>

	<field name="open_cash" class="java.math.BigDecimal"/>

